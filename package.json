{
  "name": "@glimmer/application-pipeline",
  "version": "0.6.9",
  "description": "Tooling for developing Glimmer standalone apps with ember-cli",
  "contributors": [
    "Dan Gebhardt <dan@cerebris.com>",
    "Tom Dale <tom@tomdale.net>",
    "Robbie Pitts <me@robbie-pitts.com>"
  ],
  "scripts": {
    "build": "ember build",
    "preversion": "yarn test",
    "prepublish": "yarn run build",
    "postpublish": "git push origin master --tags",
    "pretest": "ember build",
    "test": "yarn run problems && mocha dist/tests --recursive",
    "test:debug": "ember build && mocha debug dist/tests --recursive",
    "start": "ember test --server",
    "problems": "tsc -p tsconfig.json --noEmit"
  },
  "repository": "https://github.com/glimmerjs/glimmer-application-pipeline",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "keywords": [
    "glimmer",
    "ember-addon"
  ],
  "dependencies": {
    "@glimmer/compiler": "^0.23.0-alpha.11",
    "@glimmer/di": "^0.2.0",
    "@glimmer/resolution-map-builder": "^0.3.8",
    "@glimmer/resolver-configuration-builder": "^0.1.2",
    "babel-plugin-debug-macros": "^0.1.7",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "broccoli-asset-rev": "^2.4.3",
    "broccoli-babel-transpiler": "^6.0.0",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-concat": "^3.2.2",
    "broccoli-config-loader": "^1.0.0",
    "broccoli-config-replace": "^1.1.2",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-persistent-filter": "^1.3.1",
    "broccoli-rollup": "^1.3.0",
    "broccoli-sass": "^0.7.0",
    "broccoli-source": "^1.1.0",
    "broccoli-stew": "^1.5.0",
    "broccoli-string-replace": "^0.1.1",
    "broccoli-typescript-compiler": "^1.0.1",
    "broccoli-uglify-sourcemap": "^1.4.0",
    "common-tags": "^1.4.0",
    "ember-build-utilities": "^0.1.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-normalize-entity-name": "^1.0.0",
    "ember-cli-preprocess-registry": "^3.1.1",
    "ember-cli-string-utils": "^1.1.0",
    "ember-cli-valid-component-name": "^1.0.0",
    "exists-sync": "^0.0.4",
    "handlebars": "^4.0.8",
    "heimdalljs-logger": "^0.1.9",
    "lodash.defaultsdeep": "^4.6.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "silent-error": "^1.1.0",
    "walk-sync": "^0.3.1"
  },
  "devDependencies": {
    "@glimmer/build": "^0.8.1",
    "@glimmer/resolver": "^0.3.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "broccoli-test-helper": "^1.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "ember-cli": "^2.13.1",
    "ember-cli-blueprint-test-helpers": "^0.17.2",
    "mocha": "^3.4.1",
    "testdouble": "^2.1.2",
    "typescript": "^2.3.2"
